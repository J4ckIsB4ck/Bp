program procenti;

 var 
   S, P, I, T, K: real;
   
 begin
 
  writeln('Введите значение P');
 readln(P);
  writeln('Введите значение I');
 readln(I);
  writeln('Введите значение T');
 readln(T);
  writeln('Введите значение K');
 readln(K);
 
 writeln('Конечный результат S =', S*I*T/K/100,'=' );
 
 end.